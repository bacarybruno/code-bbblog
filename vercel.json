{
  "routes": [
    { "src": "/", "dest": "/api/home" },
    { "src": "/sitemap.xml", "dest": "/api/sitemap.xml" },
    { "src": "/rss.xml", "dest": "/api/rss.xml" },
    { "src": "/about", "dest": "/api/about" },
    { "src": "/blog", "dest": "/api/blog" },
    { "src": "/posts/(?<title>[^/]+)", "dest": "/api/post?title=$title" },
    { "handle": "filesystem" }
  ]
}